function FindProxyForURL(url, host)
{
// variable strings to return
var proxy_yes = "PROXY 192.168.1.1112:808";
var proxy_no = "DIRECT";
if (shExpMatch(url, "http://hot.vrs.sohu.com/.*")) { return proxy_no; }
if (shExpMatch(url, "http://vv.video.qq.com/.*")) { return proxy_no; }
if (shExpMatch(url, "http://api.letv.com/.*")) { return proxy_no; }
// Proxy anything else
return proxy_yes;
}
